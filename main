
git init

git status 

git add nume_fisier
git add nume_director
git add . <- tot ce e in directorul curent 

git diff
git diff - staged 

git commit
git commit -v
git commit -m "mesaj"

.gitigone <- ce sa fie ignorat de init

git log
git log -2 <- cele mai recente 2 commits
git log -p
git log --pretty=oneline <- pretty poate sa primeasca si alte valori
git log grapf <- output sexy 

git commit --amend <- cand ai facut deja commit, dar mai vrei sa bagi ceva in commit
git revert id_commit <- revert oentru un commit

git cloune url <- sa clonezi local un repo (url afli din git hub la optiuea clone/download)

git remote <- sa vezi remote repos
git remote add new-remote url 
git fetch origin
git merge origin/master master <- face merge din origin/master cu master branch 
git push origin master 
git remote show origin 
git pull = fetch+merge origin/master master

git tag <- list all tags 
git tag "1.0" (lightweigh tag)
git show "1.0"
git tag -a "1.1" -m "acesta e un tag annotated"  (tip de tag annotated, contine mai multe informatii)
git push origin 1.1 <- push doar la tag-ul 1.1
git push origin --tags <- push la toate tagurile
